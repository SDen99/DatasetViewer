{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/_app/fileProcessor\\.worker\\.js",
            "headers": {
                "Content-Type": "text/javascript",
                "Service-Worker-Allowed": "/"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}