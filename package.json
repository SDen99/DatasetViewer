{
	"name": "readsas7bdat",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "NODE_ENV=development vite dev",
		"build": "NODE_ENV=production vite build",
		"preview": "NODE_ENV=production vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write \"src/**/*.{js,ts,svelte,css,html,json,md}\"",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"coverage": "vitest run --coverage",
		"analyze": "depcruise --include-only '^src' --output-type dot src | dot -T svg > dependency-graph.svg",
		"docs:core": "typedoc src/lib/core/**/*.ts --exclude **/*.svelte",
		"docs:all": "typedoc"
	},
	"devDependencies": {
		"@melt-ui/svelte": "^0.86.2",
